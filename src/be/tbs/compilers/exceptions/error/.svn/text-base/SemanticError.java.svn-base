package be.tbs.compilers.exceptions.error;

/**
 * Semantic Error
 * @author gino
 */
public class SemanticError extends CompileError {

	/**
	 * Serial Version UID
	 */
	private static final long serialVersionUID = -8986591608057784903L;

	/**
	 * Construct a Semantic Error with a given reason.
	 * @param message - The reason for constructing this error.
	 */
	public SemanticError(String message) {
		super(message);
	}

	/**
	 * Construct a Semantic Error with a given reason and cause.
	 * @param message - The reason for constructing this error.
	 * @param cause - The underlying cause for this error.
	 */
	public SemanticError(String message, Throwable cause) {
		super(message, cause);
	}	
	
}
